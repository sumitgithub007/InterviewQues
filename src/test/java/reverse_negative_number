import java.math.BigInteger;
import java.util.*;
class Solution {

    public void reverse(int num)
    {
        if(num<0)
        {
            num=Math.abs(num);
            System.out.print("-");
        }

        if(num>=0 && num<10)
        {
            System.out.println(num);
            return;
        }

        else{
            System.out.print(num%10+"");
            reverse(num/10);
        }
    }


public static void main(String args[]) {

    int a[] = new int[]{1,2,3,4,5,1,1,1,1,1};
    Solution s1 = new Solution();
     s1.reverse(9548676);



}
 
}
